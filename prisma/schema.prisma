generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model TodoItem {
  id       String   @id @default(uuid())
  label    String
  createAt DateTime @default(now())
  done     Boolean  @default(false)
  isDelete Boolean  @default(false)
}
