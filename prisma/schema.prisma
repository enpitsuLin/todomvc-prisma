generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TodoItem {
  id       String   @id @default(uuid())
  label    String
  createAt DateTime @default(now())
  done     Boolean  @default(false)
  isDelete Boolean  @default(false)
}
